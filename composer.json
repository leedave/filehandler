{
    "name": "leedch/filehandler",
    "type": "library",
    "description": "PHP Classes that are designed to simplify the handling of files and folders",
    "keywords": [
        "leedch",
        "files",
        "folders",
        "phpunit"
    ],
    "homepage": "https://github.com/leedave/filehandler.git",
    "license": "MIT",
    "authors": [
        {
            "name": "David Lee",
            "email": "leed@leed.ch",
            "homepage": "https://www.leed.ch/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.0|~7.1|~7.2|~7.3|~7.4|~8.0|~8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Leedch\\Filehandler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leedch\\Filehandler\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
